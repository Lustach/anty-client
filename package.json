    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "build:staging": "vue-cli-service build --modern --dest dist/staging --mode production",
    "build:staging:surge": "vue-cli-service build --modern --dest dist/staging --mode production; surge dist/staging dolphin-anty.surge.sh"
