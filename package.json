  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:functional": "npx jest tests/functional",
    "test:unit": "gauge run tests/unit/specs",
    "start:local": "cd src/api; npm run start",
    "electron:build:mac": "vue-cli-service electron:build --mac",
    "electron:build:mac:arm": "vue-cli-service electron:build --mac --arm64",
    "electron:build:win": "vue-cli-service electron:build --win --x64",
